    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "2.8.8",
    "react-error-overlay": "6.0.11",
    "react-scripts": "5.0.1",
    "typescript": "4.9.5"
