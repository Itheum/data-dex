{
  "name": "data-dex",
  "version": "0.8.1-alpha",
  "description": "The Itheum Data DEX enables you to trade your personal data using web3 tech",
  "dependencies": {
    "@chakra-ui/icons": "2.0.13",
    "@chakra-ui/react": "2.4.3",
    "@multiversx/sdk-dapp": "2.2.9",
    "@multiversx/sdk-core": "11.2.0",
    "@multiversx/sdk-network-providers": "1.2.1",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@sentry/react": "7.24.2",
    "@sentry/tracing": "7.24.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "axios": "1.2.1",
    "dotenv": "16.0.3",
    "framer-motion": "7.6.19",
    "mime": "3.0.0",
    "moment": "2.29.4",
    "moralis-v1": "1.12.0",
    "nft.storage": "^7.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-icons": "4.7.1",
    "react-moralis": "1.4.2",
    "react-router-dom": "6.4.5",
    "react-scripts": "5.0.1",
    "web-vitals": "3.1.0"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=$(node -pe 'require(\"./package.json\").version') react-scripts start",
    "build": "REACT_APP_VERSION=$(node -pe 'require(\"./package.json\").version') react-scripts build",
    "localWinBuild": "react-scripts build",
    "localWinStart": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-flat": "npx http-server -o /flat",
    "predeploy": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Itheum/data-dex.git"
  },
  "eslintConfig": {
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "no-debugger": [
        "warn"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    },
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "16.18.6",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/react-router-dom": "5.3.3",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-config-standard": "17.0.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "2.8.1",
    "react-scripts": "5.0.1",
    "sass": "1.56.1",
    "typescript": "4.9.3"
  }
}
